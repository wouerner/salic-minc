            $3('#small-menu-button').on('click', function () {
                $3(this).find('i').text('more_horiz')
                $3('body').toggleClass('small-menu')

                if ($3('body.small-menu').is(':visible')) {
                    $3(this).find('i').text('more_horiz')
                    setCookie('menu', 'small-menu', 365)
                } else {
