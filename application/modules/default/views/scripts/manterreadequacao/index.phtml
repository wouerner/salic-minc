                    data : {
                        idPronac : idPronac,
                        idPedidoAlteracao : idPedidoAlteracao
                    },
                    success: function(data){
                        if(data.resposta){
                            //SE EXISTIR, REMOVE AS COMBOS RELACIONADAS A TECNICOS
                            $('#encaminharTecnico').remove();
                            $('#encaminharParecerista').removeClass('sumir');
