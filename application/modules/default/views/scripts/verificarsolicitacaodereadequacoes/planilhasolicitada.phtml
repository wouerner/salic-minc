<script type="text/javascript">

    function analise(functionsim, functionnao){
        $("#alert").dialog({
            resizable: true,
            title: 'Confirma',
            width:300,
            height:150,
            modal: true,
            autoOpen:false,
            buttons: {
                'Não': functionnao
                ,
                'Sim': functionsim
            }
        });
        $("#ok").dialog({
            resizable: true,
            title:'Alerta',
            width:350,
            height:150,
            modal: true,
            autoOpen:false,
            buttons: {
                'Não': function(){
                    $(this).dialog('close');
                }
            }
        });
        
        $("#alert").dialog('open');
        $(".ui-dialog-titlebar-close").remove();
    }

    function envia()
    {
        dsObservacao = $('#justificativaencaminha').val();
        if ( dsObservacao == "Digite a observação..." || dsObservacao == "") {
            $("#alert").dialog("destroy");
            $("#alert").html('Preencha a observação');
            $("#alert").dialog({
                height: 170,
                width: 300,
                modal: true,
                title: 'Alerta!',
                draggable: false,
                resizable: false,
                closeOnEscape: false,
                buttons: {
                    'Ok': function() {
                        $(this).dialog('close');
                    }
                }
            });
            $('.ui-dialog-titlebar-close').remove();
            return false;

        } else {
            $('#obervacaoDaAvaliacao').val(dsObservacao);
            $('#finalizarAvaliacaoItens').submit();
        }
    }

    $(document).ready(function(){
        $("#analise").change(function(){

            if($(this).val() == 3){
                var valida = true;
                $(".required").each(function(){
                    if($(this).attr('stavaliacao') == 'EA') {
                        valida = false;
                    }
                });

                if(valida == false){
                    $("#alertStatus").dialog({
                        resizable: true,
                        title:'Alerta',
                        width:350,
                        height:150,
                        modal: true,
                        autoOpen:false,
                        buttons: {
                            'OK': function(){
                                $(this).dialog('close');
                                    $("#analise :first").attr('selected', 'selected');
                            }
                        }
                    });
                    $("#alertStatus").dialog('open');
                    return valida;
                }

                $("#alert").html('Deseja finalizar a Análise?')
                analise(function(){
                    $("#encaminhar").dialog({
                        resizable: true,
                        title: 'Confirma',
                        width:450,
                        height:250,
                        modal: true,
                        autoOpen:false,
                        buttons: {
                            'Cancelar' : function(){
                                $("#analise").find("option[value='2']").attr('selected','selected');
                                $(this).dialog('close');
                            },
                            'Enviar': envia
                        }
                    }).dialog('open');
                    $(this).dialog('close');
                    $(".ui-dialog-titlebar-close").remove();
                
                    }, function() {
                        $("#analise").find("option[value='2']").attr('selected','selected');
                        $(this).dialog('close');
                    }
                );
                $("#alert").dialog('open');
            
            } else if($(this).val() == 2){
                $("#alert").dialog("destroy");
                $("#alert").html('Deseja alterar o status da solicitaç&atilde;o?');
                $("#alert").dialog({
