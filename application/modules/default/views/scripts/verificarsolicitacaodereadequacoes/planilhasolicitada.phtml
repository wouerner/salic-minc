                        'Sim': function() {
                            redirecionar('<?php echo $this->url(array('controller' => 'verificarsolicitacaodereadequacoes', 'action' => 'alterar-status-itens')); ?>?idpronac=<?php echo $_GET['idPronac']; ?>');
