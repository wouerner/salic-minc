    function JSEncaminharParaAnalise(idPronac,idOrgaoDestino,tipoFiltro,ocultarJustificativa){
        console.log('teste');
        $('html').css('overflow', 'hidden');
        $("body").append("<div id='divDinamica'></div>");
        $("#divDinamica").html("");
        $('#divDinamica').html("<br><br><center>Carregando dados...</center>");
