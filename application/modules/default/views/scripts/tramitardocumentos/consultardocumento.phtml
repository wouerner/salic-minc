<script type="text/javascript">
    $(document).ready(function(){

        $('.btn_cancelar').click(function(){
            var objeto = $(this),
                idHistorico = objeto.attr('idHistorico');
            $("#msgAlerta").dialog("destroy");
            $("#msgAlerta").html("Deseja realmente cancelar a tramitação?");
            $("#msgAlerta").dialog({
                resizable: false,
                title: 'Alerta!',
                width:340,
                height:160,
                modal: true,
                buttons : {
                    'Não' : function(){
                        $(this).dialog('close');
                    },
                    'Sim' : function(){
                        $.ajax({
                            type: 'POST',
                            url: '<?php echo $this->url(array('controller' => 'tramitardocumentos', 'action' => 'cancelar-tramitacao')); ?>',
                            data: {
                                idHistorico: idHistorico
                            },
                            success: function(data){
                                if(data.resposta){
                                    $("#msgAlerta").dialog("destroy");
                                    $("#msgAlerta").html('<center><br /><img src="<?php echo $this->baseUrl(); ?>/public/img/ajax.gif" alt="carregando"><br />Por favor, aguarde!!</center>');
                                    $("#msgAlerta").dialog({
                                        resizable: false,
                                        title: 'Alerta!',
                                        width:300,
                                        height:150,
                                        modal: true
                                    });
                                    $('.ui-dialog-titlebar-close').remove();

                                    window.setInterval(function() {
                                        objeto.parent().parent().remove();
                                        $("#msgAlerta").dialog("destroy");
                                    }, 1500);
                                } else {
                                    $("#msgAlerta").dialog("destroy");
                                    $("#msgAlerta").html('Não foi possível cancelar a tramitação!');
                                    $("#msgAlerta").dialog({
                                        resizable: false,
                                        title: 'Alerta!',
                                        width:320,
                                        height:160,
                                        modal: true,
                                        buttons: {
                                            'Ok':function(){
                                                $(this).dialog('close'); // fecha a modal
                                            }
                                        }
                                    });
                                    $('.ui-dialog-titlebar-close').remove();
                                }
                            },
                            dataType : 'json'
                        });
                    }
                }
            });
            $('.ui-dialog-titlebar-close').remove();
        });

    });

    function visualizarTextoModal(text){
        $("#visualizarTextoModal").dialog("destroy");
        $("#visualizarTextoModal").html(text);
        $("#visualizarTextoModal").dialog({
            resizable: true,
            modal: true,
            autoOpen:true,
            EscClose:false,
            buttons: {
                'Ok':function(){
                    $(this).dialog('close'); // fecha a modal
                }
            }
        });
        $("#visualizarTextoModal").dialog('open');
    }
</script>

