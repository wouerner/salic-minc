                resizable: true,
                title: 'Observa&ccedil;&otilde;es',
                width:350,
                modal: true,
                buttons : {
                    'OK' : function(){
                        $(this).dialog('close');
                    }
                }
            });
            $('.ui-dialog-titlebar-close').remove();
            return false;
        });
    });
