<script type="text/javascript">
    $(document).ready(function () {
        CKEDITOR.replaceAll('textocomposto');
        acertaLayout();
        somaEmpregos();
        limparCampos();

        $('.qtEmprego').blur(function () {
            somaEmpregos();
        });

        $('#addAnexo').click(function () {
            var divAnexos = $('#divAnexos');
            var div = $('<div></div>').css('marginLeft', '200px').appendTo(divAnexos);
            var file = $('<input />').attr('type', 'file').attr('name', 'arquivo[]').addClass('input_simples').appendTo(div);
            var excluir = $('<input />').attr('type', 'button').addClass('btn_excluir').click(function () {
                div.remove();
            }).appendTo(div);

        });

        $('#Aprovar').click(function () {
            $('#stAprovar').val(1);
            $('#stAvaliacao').val(1);
            var este = this;
            var nomeJanelaAlerta = janelaObj({
                parametros: {
                    width: 400,
                    autoOpen: false,
                    resizable: false,
                    modal: true,
                    buttons: {
                        'N\u00e3o': function () {
                            $(this).dialog('close');
                        },
                        Sim: function () {
                            preencheTextarea();
                            if (validarFormulario('#formParecerCoordenador', 'Dados obrigat&oacute;rios não informados!')) {
                                $('#formParecerCoordenador').submit();
                            }
                            $(this).dialog('close');
                        }
                    }
                },
                removerBtFechar: true,
                title: 'Alerta'
            });
            nomeJanelaAlerta.divConteudo.html('Tem certeza que deseja aprovar?');
            nomeJanelaAlerta.abrirJanela();
            return false;
        });

        $('#DevolverTecnico').click(function () {
            $('#stAprovar').val(0);
            $('#stAvaliacao').val(0);
            var nomeJanelaAlerta = janelaObj({
                parametros: {
                    width: 400,
                    autoOpen: false,
                    resizable: false,
                    modal: true,
                    buttons: {
                        'N\u00e3o': function () {
                            $(this).dialog('close');
                        },
                        Sim: function () {
                            preencheTextarea();


                            if (validarFormulario('#formParecerCoordenador', 'Dados obrigatórios n&atilde;o informados!')) {
                                $('#formParecerCoordenador').submit();
                            }
                            $(this).dialog('close');
                        }
                    }
                },
                removerBtFechar: true,
                title: 'Alerta'
            });
            nomeJanelaAlerta.divConteudo.html(' sfdgfgh Deseja devolver para o t&eacute;cnico? <table><tr><th colspan="4">Justificativa:</th></tr><tr><td colspan="4"><textarea null="false" id="justTecnico" name="justTecnico" cols="1" rows="3" style="width: 80%;"><?php if (isset($relatorio->dsJustificativaDevolucao)) echo $relatorio->dsJustificativaDevolucao;?></textarea></td></tr></table>');
            alert('aeaeaea');
            nomeJanelaAlerta.abrirJanela();
            return false;
        });

        $('#Salvar').click(function () {
            $('#stAprovar').val(0);
            $('#stAvaliacao').val(1);
            $('#formParecerCoordenador').submit();
        });

        $('#Cancelar').click(function () {
            location.href = "<?php echo $this->url(array('controller' => 'pesquisarprojetofiscalizacao', 'action' => 'grid'));?>";
        });

        $('.btn_excluir').click(function () {
            var este = this;
            var nomeJanelaAlerta = janelaObj({
                parametros: {
                    width: 400,
                    autoOpen: false,
                    resizable: false,
                    modal: true,
                    buttons: {
                        'N\u00e3o': function () {
                            $(this).dialog('close');
                        },
                        Sim: function () {
                            var resp;
                            if ($(este).attr('idArquivoFiscalizacao') != undefined && $(este).attr('idArquivo') != undefined) {
                                resp = buscarJson('<?php echo $ExcluirHref;?>', {
                                    idArquivoFiscalizacao: $(este).attr('idArquivoFiscalizacao'),
                                    idArquivo: $(este).attr('idArquivo')
                                });
                            }
                            if (resp.result) {
                                $(este).parent().parent().remove();
                            }
                            janelaAlerta(resp.mensagem);

                            $(this).dialog('close');
                        }
                    }
                },
                removerBtFechar: true,
                title: 'Alerta'
            });
            nomeJanelaAlerta.divConteudo.html('Deseja excluir?');
            nomeJanelaAlerta.abrirJanela();
        });
    });

    function preencheTextarea() {
        $('.textocomposto').each(function () {
            $(this).val(CKEDITOR.instances[$(this).attr('id')].getData());
        });
    }

    function acertaLayout() {
        $('input[type!=submit]|[type!=button]').addClass('input_simples');
        $('textarea').addClass('textarea_simples');
        $('select[id!=perfilXgrupo]').addClass('select_simples');
    }

    function somaEmpregos() {
        var empregoDireto = 0;
        var empregoIndireto = 0;

        if (!isNaN(parseInt($('#qtEmpregoDireto').val()))) {
            empregoDireto = parseInt($('#qtEmpregoDireto').val());
        }
        if (!isNaN(parseInt($('#qtEmpregoIndireto').val()))) {
            empregoIndireto = parseInt($('#qtEmpregoIndireto').val());
        }

        $('#totalEmprego').val(parseInt(empregoDireto) + parseInt(empregoIndireto));

    }

    function janelaAlerta(mensagem, funcaoAdcional) {
        if (funcaoAdcional == undefined) {
            funcaoAdcional = function () {
            }
        }
        var nomeJanelaAlerta = janelaObj({
            parametros: {
                width: 400,
                autoOpen: false,
                resizable: false,
                modal: true,
                buttons: {
                    OK: function () {
                        funcaoAdcional();
                        $(this).dialog('close');
                    }
                }
            },
            removerBtFechar: true,
            title: 'Alerta'
        });
        nomeJanelaAlerta.divConteudo.html(mensagem);
        nomeJanelaAlerta.abrirJanela();

        return nomeJanelaAlerta;
    }

    function janelaObj(dados) {
        var divConteudo = $('<div></div>')
            .attr('title', dados.title)
            .appendTo('body');
        var novaJanela = {
            divConteudo: divConteudo,
            removerBtFechar: true,
            parametros: { autoOpen: false },
            iniciarJanela: function (dados) {
                this.refineParametrosObj(dados);

                this.divConteudo.dialog(this.parametros);
            },
            abrirJanela: function () {
                this.divConteudo.dialog('open');
                if (this.removerBtFechar)
                    $('.ui-dialog-titlebar-close').remove();
            },
            fecharJanela: function () {
                this.divConteudo.dialog('close');
                this.divConteudo.remove();
            },
            refineParametrosObj: function (data) {
                if (data != undefined)
                    for (var j in data) {
                        this[j] = data[j];
                    }
            }
        }
        novaJanela.iniciarJanela(dados);
        return novaJanela;
    }

    function requisicaoAjaxObj() {
        var ajaxObj = {
            pagina: '',
            parametros: {},
            type: 'post',
            dataType: '',
            resposta: '#conteudo',
            async: true,
            funcaoRetorno: function (resposta) {
                $(this.resposta).html(resposta);
            },
            executar: function (dados) {
                this.refineParametrosObj(dados);
                var esteObj = this;
                if (this.resposta != undefined && this.resposta != '')
                    $(this.resposta).html('<img src="<?php echo $this->baseUrl(); ?>/public/img/ajax.gif" alt="carregando"><br/><br/>Carregando...<br>Por Favor, aguarde!!');
                $.ajax({
                    type: esteObj.type,
                    url: esteObj.pagina,
                    data: esteObj.parametros,
                    async: esteObj.async,
                    success: function (resp) {
                        esteObj.funcaoRetorno(resp);
                    }
                    , dataType: esteObj.dataType
                });
            },
            refineParametrosObj: function (data) {
                if (data != undefined)
                    for (var j in data) {

                        this[j] = data[j];
                    }
            }
        }
        return ajaxObj;
    }

    function buscarJson(pagina, dados) {
        var retorno = '';
        var select = requisicaoAjaxObj();
        select.executar({
            pagina: pagina,
            parametros: dados,
            resposta: undefined,
            async: false,
            funcaoRetorno: function (resposta) {
                retorno = resposta;
            }
            , dataType: 'json'
        });
        return retorno;
    }

    function validarFormulario(form, mensagem) {
        var validar = false;
        if (mensagem == undefined) {
            mensagem = '';
        }
        $(form).find('[null=false]').each(function () {
            if ($(this).attr('type') == 'radio') {
                var validaRadio = true;
                $(form + ' [name=' + $(this).attr('name') + ']').each(function () {
                    if ($(this).attr('checked'))
                        validaRadio = false;
                });
                if (validaRadio)
                    validar = true;
            } else {
                var texto = $.trim($(this).val());
                var texto = texto.replace(/(<.*?>|\s+)/g, '');
                if (texto.length == 0) {
                    validar = true;
                }
            }
        });

        if (validar) {
            janelaAlerta(mensagem);
            return false;
        } else
            return true;
    }

    function limparCampos() {
        $('#formParecerCoordenador').find('textarea').each(function () {
            var este = $(this).val().replace(/( |\n)/g, '');
            if (!este.length) {
                $(this).val(este);
            }
        });
    }
</script>
