<script type="text/javascript">
    $(document).ready(function(){
        $('.iconeJustificativa').click(function(){
            var icone = $(this),
                valor = icone.siblings().val();

            $("#justificativaModalVisualizar").dialog("destroy");
            $('#justificativaModalVisualizar').html(valor);
            $("#justificativaModalVisualizar").dialog({
                resizable: true,
                title: 'Justificativa cadastrada!',
                //width:520,
                //height:300,
                modal: true,
                buttons : {
                    'OK' : function(){
                        $(this).dialog('close');
                    }
                }
            });
            $('.ui-dialog-titlebar-close').remove();
        });

        $('.btn_imprimir').click(function(){
            $('#formImprimir').submit();
        });

        $('.div_expandir_itens_orcamentarios').click(function(){
            var id = $(this).attr('id'),
                expandido = $(this).attr('expandido');

            if(expandido == '0'){
                $.ajax({
                    type: 'POST',
                    url: '<?php echo $this->url(array('module' => 'comprovacao-objeto', 'controller' => 'comprovarexecucaofisica', 'action' => 'buscar-dados-itens')); ?>',
                    data: {
                        idPlanilhaAprovacao: id,
                        idpronac: <?php echo $this->idPronac; ?>
                    },
                    success: function(data){
                        var recebe = $('#tabela_'+id);
                        if(data.resposta){
                            for ( i in data.conteudo){
                               recebe.append('<tr><td>'+data.conteudo[i].Fornecedor+'</td><td align="center">'+data.conteudo[i].DtPagamento+'</td><td>'+data.conteudo[i].tpDocumento+'</td><td>'+data.conteudo[i].nrComprovante+'</td><td>'+data.conteudo[i].tpFormaDePagamento+'</td><td align="center">'+data.conteudo[i].dtEmissao+'</td><td align="right">'+data.conteudo[i].vlComprovacao+'</td><td><a href="<?php echo $this->url(array('module' => 'default', 'controller' => 'upload', 'action' => 'abrir')); ?>?id='+data.conteudo[i].idArquivo+'">'+data.conteudo[i].nmArquivo+'</a></td></tr>');
                            }
                        }
                    },
                    dataType : 'json'
                });
                $(this).attr('expandido',1);
            }
        });
    });
</script>

