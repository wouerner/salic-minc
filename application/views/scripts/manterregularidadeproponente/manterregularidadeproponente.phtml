    function addDia(dtEmissao,tipo,campo){
      
        if ( dtEmissao.length == 10 ){
      
            $('html').css('overflow', 'hidden');
            $("body").append("<div id='divDinamica'></div>");
            $("#divDinamica").html("");
            $('#divDinamica').html("<br><br><center>Calculando data! Aguarde, por favor...</center>");
            var title = 'Alerta';
            $("#divDinamica").dialog('close');
            $("#divDinamica").dialog({
                width:300,
                height:200,
                modal: true,
                autoOpen:true,
                draggable:false,
                title: title
            });
        
            $.ajax({
                url : '<?php echo $this->url(array('controller' => 'manterregularidadeproponente', 'action' => 'calculadataemissaoduracao')); ?>',
                data :
                    {
                    dtEmissao : dtEmissao,
                    tipo: tipo
                },
                success: function(data){
                    
                    if( data.error ){
                        $("#divDinamica").remove();
                        if( campo == "federal" ){
                            $('#txtDtFutSpan').html('<br><font color="red">'+data.msg+'</font>');
                            $('#txtDtFut').val('');
                        }
                        if( campo == "fgts" ){
                            $('#txtDtFutFGTSSpan').html('<br><font color="red">'+data.msg+'</font>');
                            $('#txtDtFutFGTS').val('');
                        }
                        if( campo == "inss" ){
                            $('#txtDtFutINSSSpan').html('<br><font color="red">'+data.msg+'</font>');
                            $('#txtDtFutINSS').val('');
                        }
                    }else{
                        $("#divDinamica").remove();
                        if( campo == "federal" ){
                            $('#txtDtFutSpan').html('');
                        }
                        if( campo == "fgts" ){
                            $('#txtDtFutFGTSSpan').html('');
                        }
                        if( campo == "inss" ){
                            $('#txtDtFutINSSSpan').html('');
                        }
                    }
                    
                    if ( data.existe )
                    {
                        $('#'+campo).val(data.data);
                        if ( tipo == 1 ){
                            $('#quitacaoFederalDias').val('180');
                        }else if( tipo == 3 ){
                            $('#quitacaoFGTSDias').val('29');
                        }else{
                            $('#quitacaoINSSDias').val('180');
                        }
                        $("#divDinamica").remove();
                    }
                    colorirLinha();
                },
                dataType : 'json'
            });
            $('.ui-dialog-titlebar-close').remove();
            
        }
    }

    function buscaPronac(idPronac, idCampo){

        var valorCampo = $('#'+idCampo).val();
        if ( valorCampo.length >= 6 ){
            if ( idPronac != "" ) {
    
                $('html').css('overflow', 'hidden');
                $("body").append("<div id='divDinamica'></div>");
                $("#divDinamica").html("");
                $('#divDinamica').html("<br><br><center>Verificando PRONAC! Aguarde, por favor...</center>");
            
                var title = 'Alerta';
                $("#divDinamica").dialog('close');
                $("#divDinamica").dialog({
                    width:300,
                    height:200,
                    modal: true,
                    autoOpen:true,
                    draggable:false,
                    title: title
                });
                $.ajax({
                    url : '<?php echo $this->url(array('controller' => 'manterregularidadeproponente', 'action' => 'buscapronac')); ?>',
                    data :
                        {
                        idPronac : idPronac
                    },
                    success: function(data){
                        if ( !data.existe ){
                            $('#' + idCampo).val('');
                            $("#divDinamica").remove();
                            $('#' + idCampo + 'Span' ).html('<br><font color="red">Pronac inexistente</br>');
                        }else{
                            $("#divDinamica").remove();
                            $('#' + idCampo + 'Span' ).html('');
                            return true;
                        }
                    },
                    dataType : 'json'

                });
                $('.ui-dialog-titlebar-close').remove();
            }
        }
    }
