    function colorirLinha(){
        
        var dtAtual = "<?php echo date("Ymd"); ?>";
        var dtTmp = null;
        var arrTmp = null;
        
        //DATA FEDERAL
        if($("#federal").val().length == 10){
            var dtValidadeFederal = $("#federal").val();
            dtTmp = dtValidadeFederal;
            arrTmp = dtTmp.split("/");    
            dtValidadeFederal = arrTmp[2]+arrTmp[1]+arrTmp[0];
        }else{
            var dtValidadeFederal = dtAtual;
        }   
        
        if(dtValidadeFederal < dtAtual){
            $("#linhaFederal").addClass("vencida");
            $("#linhaFederal").addClass("vencidaInput");
        }else{
            $("#linhaFederal").removeClass("vencida");
            $("#linhaFederal").removeClass("vencidaInput");
        }
         
        //DATA FGTS
        dtTmp = null;
        arrTmp = null;
        if($("#fgts").val().length == 10){
            var dtValidadeFgts = $("#fgts").val();
            dtTmp = dtValidadeFgts;
            arrTmp = dtTmp.split("/");    
            dtValidadeFgts = arrTmp[2]+arrTmp[1]+arrTmp[0];
        }else{
            var dtValidadeFgts = dtAtual;
        }
        
        if(dtValidadeFgts < dtAtual){
            $("#linhaFgts").addClass("vencida");
            $("#linhaFgts").addClass("vencidaInput");
        }else{
            $("#linhaFgts").removeClass("vencida");
            $("#linhaFgts").removeClass("vencidaInput");
        }
            
        //DATA INSS
        dtTmp = null;
        arrTmp = null;
        if($("#inss").val().length == 10){
            var dtValidadeInss = $("#inss").val();
            dtTmp = dtValidadeInss;
