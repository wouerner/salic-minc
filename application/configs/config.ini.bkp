[default]
; Library
includePaths.library = APPLICATION_PATH "/../library"
autoloaderNamespaces[] = "Minc"
		       
; Options for Zend_View
resources.view.encoding = "ISO-8859-1"
resources.view.escape = "htmlentities"
resources.view.basePath = APPLICATION_PATH "/views"
resources.view.helperPath.MinC_View_Helper = APPLICATION_PATH "/../library/MinC/View/Helper"

[conexao_01:default]
db.adapter = PDO_MSSQL
db.params.host = mssql_desenv 
db.params.username = usuarios_internet 
db.params.password = 753159
db.params.dbname = TABELAS
resources.db.params.charset = "utf8 "
resources.db.params.driver_options.1002 = "SET NAMES utf8"
resources.db.isDefaultTableAdapter = true

